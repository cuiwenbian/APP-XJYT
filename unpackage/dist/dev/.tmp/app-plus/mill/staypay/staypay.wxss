.shade {
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, 0.4);
	position: fixed;
	left: 0;
	top: 0;
	z-index: 99;
}
.pop {
	width: 550rpx;
	height: 300rpx;
	margin: 500rpx auto;
	padding: 0 60rpx;
	box-sizing: border-box;
	background: #fff;
	border-radius: 10rpx;
}
.pop-title {
	height: 180rpx;
	line-height: 180rpx;
	text-align: center;
	font-size: 26rpx;
}
.pops {
	height: 100rpx;
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
