
.height {
	height: var(--status-bar-height);
	background-color: #121212;
}
.com{
	width:100rpx;
	height:100rpx;
	display: block;
	margin:100rpx auto 20rpx;
}
.wait{
	text-align: center;
	line-height: 100rpx;
	font-weight: bold;
}
.num{
	text-align: center;
	line-height: 50rpx;
	color:#333;
	font-size: 30rpx;
}
.add{
	width:100%;
	height:auto;
	margin-top: 50rpx;
	padding:0 48rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	overflow:hidden;
}
.tran_add{
	float: left;
	width:25%;
	line-height: 80rpx;
	font-size: 28rpx;
}
.tran_detail{
	float: right;
	width:75%;
	line-height: 80rpx;
	font-size: 28rpx;
	word-break:break-all;
	word-wrap:break-word;
}
.next{
	margin: 150rpx auto;
	width:654rpx;
	height:90rpx;
	background: #0A1117;
	border-radius: 80rpx;
	text-align: center;
	line-height: 90rpx;
	color: #fff;
}

