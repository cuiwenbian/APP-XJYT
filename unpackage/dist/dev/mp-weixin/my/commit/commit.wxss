
.height {
    height: 25px;
    background-color: #121212;
}
.com {
    width: 100rpx;
    height: 100rpx;
    display: block;
    margin: 100rpx auto 20rpx;
}
.wait {
    text-align: center;
    line-height: 100rpx;
    font-weight: bold;
}
.num {
    text-align: center;
    line-height: 50rpx;
    color: #333;
    font-size: 30rpx;
}
.add {
    width: 100%;
    height: auto;
    margin-top: 50rpx;
    padding: 0 48rpx;
    box-sizing: border-box;
    overflow: hidden;
}
.tran_add {
    float: left;
    width: 25%;
    line-height: 80rpx;
    font-size: 28rpx;
}
.tran_detail {
    float: right;
    width: 75%;
    line-height: 80rpx;
    font-size: 28rpx;
    word-break: break-all;
    word-wrap: break-word;
}
.next {
    margin: 100rpx auto 0;
    width: 690rpx;
    height: 88rpx;
    background: #0a1117;
    border-radius: 80rpx;
    text-align: center;
    line-height: 88rpx;
    color: #fff;
    font-size: 30rpx;
}

