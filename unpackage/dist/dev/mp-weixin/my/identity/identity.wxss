.submit-btn-wrap {
  padding-top: 130rpx;
}
.submit-btn-wrap .submit-btn {
    width: 511rpx;
    height: 98rpx;
    margin: 0 auto;
    line-height: 98rpx;
    text-align: center;
    color: #fff;
    font-size: 30rpx;
    border-radius: 50rpx;
    box-shadow: 0 0 15rpx 15rpx rgba(205, 247, 235, 0.3);
    background-color: #02d597;
    -webkit-transition: all .2s;
    transition: all .2s;
}
.submit-btn-wrap .submit-btn:active {
      background-color: rgba(2, 213, 151, 0.85);
}
.submit-btn-wrap .submit-btn-disable {
    width: 511rpx;
    height: 98rpx;
    margin: 0 auto;
    line-height: 98rpx;
    text-align: center;
    color: #fff;
    font-size: 30rpx;
    border-radius: 50rpx;
    background-color: rgba(2, 213, 151, 0.4);
}
.shade {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 999;
}
.pop {
  width: 500rpx;
  height: 250rpx;
  margin: 450rpx auto 0;
  background: #fff;
  border-radius: 20rpx;
}
.pop-title {
  text-align: center;
  font-size: 32rpx;
  color: #121212;
  line-height: 150rpx;
}
.pop-btn {
  width: 158rpx;
  height: 66rpx;
  margin: 0 auto;
  border-radius: 50rpx;
  line-height: 66rpx;
  font-size: 26rpx;
  color: #fff;
  background-image: -webkit-linear-gradient(left, #01c774, #01dda9);
  background-image: linear-gradient(to right, #01c774, #01dda9);
  text-align: center;
}

