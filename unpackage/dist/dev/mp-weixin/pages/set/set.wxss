}
.header .back {
    width: 160rpx;
    height: 88rpx;
    position: absolute;
    z-index: 10;
}
.header .back-bg {
    width: 24rpx;
    height: 24rpx;
    border-top: 3rpx solid #031024;
    border-left: 3rpx solid #031024;
