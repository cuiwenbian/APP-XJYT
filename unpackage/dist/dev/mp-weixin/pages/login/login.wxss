	/* margin-right: 64rpx; */
	border-bottom: 2rpx solid #ebebeb;
	padding: 20rpx 30rpx;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	-webkit-transition: all 0.2s ease;
	transition: all 0.2s ease;
	position: relative;
}
.bt-tab-active {
	color: #01c777;
	font-weight: bolder;
}
